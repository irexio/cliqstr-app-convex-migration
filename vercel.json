{
  "version": 2,
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "functions": {
    "src/app/api/**/*": {
      "maxDuration": 10
    },
    "src/app/auth/**/*": {
      "maxDuration": 10
    },
    ".next/server/app/api/**/*": {
      "maxDuration": 10
    },
    ".next/server/app/auth/**/*": {
      "maxDuration": 10
    },
    ".next/server/app/parent/**/*": {
      "maxDuration": 10
    },
    ".next/server/app/account/**/*": {
      "maxDuration": 10
    },
    ".next/server/app/profile/**/*": {
      "maxDuration": 10
    },
    ".next/server/app/choose-plan/**/*": {
      "maxDuration": 10
    },
    ".next/server/app/verify-card/**/*": {
      "maxDuration": 10
    }
  },
  "env": {
    "NEXT_PRIVATE_STANDALONE": "true",
    "NEXT_FORCE_EDGE_IMAGES": "true",
    "NEXT_PRIVATE_PREBUNDLED_REACT": "true"
  }
}
