{
  "extends": ["next/core-web-vitals"],
  "ignorePatterns": [
    "node_modules/",
    ".next/",
    "public/",
    "src/generated/",
    "*.js",
    "*.cjs",
    "*.mjs",
    "*.wasm.js"
  ],
  "rules": {
    "@typescript-eslint/no-unused-expressions": "off",
    "@typescript-eslint/no-this-alias": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "no-unused-vars": "off",
    "no-unused-expressions": "off"
  }
}
