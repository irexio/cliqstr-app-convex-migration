{
  "name": "cli",
  "projectType": "application",
  "root": ".",
  "sourceRoot": "app",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{projectRoot}/.next"],
      "options": {
        "root": ".",
        "outputPath": ".next"
      }
    },
    "dev": {
      "executor": "@nx/next:dev",
      "options": {
        "root": "."
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["app/**/*.ts", "app/**/*.tsx"]
      }
    }
  }
}
